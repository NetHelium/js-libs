{
  "name": "@net-helium/hc-forms",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Intégration de formulaires Hélium Connect sur vos pages web",
  "author": {
    "name": "Net Hélium",
    "url": "https://net-helium.fr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NetHelium/js-libs.git",
    "directory": "packages/hc-forms"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "check:types": "tsc --emitDeclarationOnly false --noEmit",
    "clean": "rm -rf dist",
    "build:source": "./scripts/build.js",
    "build:types": "tsc && mv dist/index.d.ts dist/hc-forms.d.ts",
    "build": "pnpm clean && pnpm build:source && pnpm build:types"
  },
  "main": "./dist/hc-forms.min.js",
  "types": "./dist/hc-forms.d.ts",
  "files": ["dist"],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "catalog:",
    "commander": "catalog:",
    "esbuild": "catalog:",
    "tslib": "catalog:",
    "typescript": "catalog:"
  },
  "keywords": ["hélium connect", "net hélium", "form", "formulaire", "intégration", "crm"],
  "license": "MIT",
  "packageManager": "pnpm@9.7.0",
  "engines": {
    "node": ">=20.16.0",
    "pnpm": ">=9.7.0"
  }
}
