{
  "typescript.tsdk": "node_modules/typescript/lib",
  "biome.enabled": true,
  "cSpell.enabled": true,
  "prettify-ts.maxProperties": 10,
  "prettify-ts.maxCharacters": 5000,
  "prettify-ts.typeIndentation": 2,
  "editor.rulers": [
    {
      "column": 100,
      "color": "#92929266"
    }
  ],
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome",
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome",
    "editor.formatOnSave": true
  }
}
