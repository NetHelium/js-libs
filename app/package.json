{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist && rm -rf public/js",
    "dev:types": "tsc-watch --noClear",
    "dev:source": "./scripts/build.ts --watch",
    "dev:server": "eleventy --serve --incremental",
    "dev": "pnpm clean && concurrently -n \"source,types,server\" \"pnpm dev:source\" \"pnpm dev:types\" \"pnpm dev:server\"",
    "build:source": "./scripts/build.ts",
    "build:types": "tsc",
    "build:site": "eleventy",
    "build": "pnpm clean && pnpm build:source && pnpm build:types && pnpm build:site"
  },
  "dependencies": {
    "@net-helium/hc-forms": "workspace:",
    "@net-helium/ui": "workspace:"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "catalog:",
    "@types/node": "catalog:",
    "@11ty/eleventy": "3.0.0-beta.1",
    "commander": "catalog:",
    "concurrently": "catalog:",
    "esbuild": "catalog:",
    "tsc-watch": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  },
  "license": "MIT",
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.5.0"
  }
}
